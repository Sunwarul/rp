{
    "version": "1.0.0",
    "name": "nuxtit/rp",
    "description": "Role and Permissions Management Package for Laravel Including UI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "nuxtit",
            "email": "nuxtit.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "spatie/laravel-permission": "^6.16"
    },
    "autoload": {
        "psr-4": {
            "NuxtIt\\RP\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NuxtIt\\RP\\Providers\\RPServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

